@model TimetableSys_T17.Models.RequestModel
@{
    ViewBag.Title = "Submit Request";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*@Styles.Render("~/Content/multiselectcss");*@

<b> Do not remove - READ COMMENTS </b>

<!-- ERROR TRAPPING REQUIRED - If user does park then room, it should then filter building. --> 
