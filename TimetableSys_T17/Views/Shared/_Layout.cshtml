<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
   
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script src="~/Scripts/jquery-{version}.js"></script>
    <script src="~/Scripts/jquery.unobtrusive*"></script>
    <script src="~/Scripts/jquery.validate*"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/ttablejs.js"></script>
    <script src="~/Scripts/bootstrap-multiselect.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/accessibility.js"></script>
    <script src="~/Scripts/modernizr-*"></script>
    <script src="~/bundles/jqueryval"></script>
    <link rel="stylesheet" href="/resources/demos/style.css">
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <link rel="stylesheet" href="~/Content/multiselectcss">
    <link rel="stylesheet" href="~/Content/site.css">
    <link rel="stylesheet" href="~/Content/jquery-ui.css">
    <link rel="stylesheet" href="~/Content/jquery-ui.structure.min.css">
    <link rel="stylesheet" href="~/Content/jquery-ui.theme.css">
    <link rel="stylesheet" href="~/Content/bootstrap-multiselect.css">
    <link rel="stylesheet" href="~/Content/.css">
</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top"><img class="logo" src="~/Content/images/lborologo.jpg" alt="Image" />

     <div style="float:right; padding:2px;padding-right:1%;">
             <p style="float: left; color: white;">Font Size:
     <button  style="width:20px" id="decrease">-</button>
     <button  style=" width:20px;" id="increase">+</button></p>
     
         <br />
          <br />

  <input style="float: none; width: auto;"  type="button" onclick="invertColours();" value="Invert Colours"/>
    </div>

     <div class="container">
            <div class="navbar-header">                
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Loughborough University Timetabling Website", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Create Request", "Create Request", "Create Request")</li>
                    <li>@Html.ActionLink("View Request", "View Request", "View Request")</li>
                </ul>
                @*@Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </div>
    <br /><br /><br /><br /><br /><br />
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div> 
    @RenderSection("scripts", required: false)


 <h1 style="font-size:50 px">   testing la</h1>
</body>
</html>
